diccionario1 = {"juan"}
diccionarionotas1 = {3, 8, 9, 7, 6}

diccionario2 = {"tatiana"}
diccionarionotas2 = {5, 8, 9, 7, 6}

diccionario3 = {"maria"}
diccionarionotas3 = {8, 8, 9, 7, 6}

diccionario4 = {"hernandez"}
diccionarionotas4 = {1, 8, 9, 7, 6}

diccionario5 = {"luis"}
diccionarionotas5 = {2, 8, 9, 7, 6}

print ("Lista de estudiantes:", diccionario1)
print ("Notas de los estudiantes:", diccionarionotas1)
print ("Promedio de notas:", sum(diccionarionotas1) / len(diccionarionotas1)) 

print ("Lista de estudiantes:", diccionario2)
print ("Notas de los estudiantes:", diccionarionotas2)
print ("Promedio de notas:", sum(diccionarionotas2) / len(diccionarionotas2)) 

print ("Lista de estudiantes:", diccionario3)
print ("Notas de los estudiantes:", diccionarionotas3)
print ("Promedio de notas:", sum(diccionarionotas3) / len(diccionarionotas3)) 

print ("Lista de estudiantes:", diccionario4)
print ("Notas de los estudiantes:", diccionarionotas4)
print ("Promedio de notas:", sum(diccionarionotas4) / len(diccionarionotas4)) 

print ("Lista de estudiantes:", diccionario5)
print ("Notas de los estudiantes:", diccionarionotas5)
print ("Promedio de notas:", sum(diccionarionotas5) / len(diccionarionotas5)) 
